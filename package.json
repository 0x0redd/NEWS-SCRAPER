{
  "name": "csc-api-server",
  "version": "1.0.0",
  "description": "Backend API server for CSC landing page - handles email sending and other backend services",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "scraper": "node news-scraper.js",
    "scraper:once": "node news-scraper.js --once",
    "scraper:schedule": "node news-scraper.js --schedule",
    "pm2:start": "pm2 start index.js --name csc-api",
    "pm2:stop": "pm2 stop csc-api",
    "pm2:restart": "pm2 restart csc-api",
    "pm2:logs": "pm2 logs csc-api",
    "pm2:status": "pm2 status",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "api",
    "express",
    "email",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "googleapis": "^129.0.0",
    "helmet": "^7.1.0",
    "nodemailer": "^7.0.10"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "pm2": "^5.4.3"
  }
}
